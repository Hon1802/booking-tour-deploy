{
  "version": 2,
  "builds": [
    {
      "src": "./src/server.js",
      "use": "@now/node"
    },
    {
      "src": "src/router/web.js",
      "use": "@now/node"
    },
    {
      "src": "src/router/auth.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/router/web/(.*)",
      "dest": "src/router/web.js"
    },
    {
      "src": "/router/auth/(.*)",
      "dest": "src/router/auth.js"
    },
    {
      "src": "/(.*)",
      "dest": "src/server.js"
    }
  ]
}
